{
  "allowed_extensions": [
    ".py", ".java", ".cpp", ".c", ".h", ".hpp", ".js", ".ts", ".html", ".css",
    ".sql", ".yaml", ".yml", ".xml", ".json", ".md", ".txt", ".cs",
    ".shader", ".compute", ".cginc", ".go", ".rs", ".lua"
  ],

  "ignore_dirs": [
    ".git", ".idea", ".vscode", "__pycache__", "venv", "env", "node_modules", ".DS_Store",
    "build", "dist", "bin", "obj", "target",
    "gradle", ".gradle", "Library", "Temp", "Logs", "UserSettings",
    "Drivers", "Middlewares", "CMSIS", "MDK-ARM", "EWARM", "DebugVals", "Docs", "Doc"
  ],

  "ignore_prefixes": [
    "stm32f4xx_it", "system_stm32", "stm32f4xx_hal_conf",
    "stm32f4xx_hal_msp", "sysmem", "syscalls", "FreeRTOSConfig",
    "jquery", "bootstrap"
  ],

  "ignore_files": [
    "CodeFeeder_GUI.py",
    "install_menu.py",
    "package-lock.json",
    "yarn.lock"
  ],

  "version": [
    "AI CodeFeeder v2.0 (Pipeline Edition)",
    "Refactored for Efficiency"
  ]
}